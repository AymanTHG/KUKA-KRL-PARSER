<Window x:Class="KUKA_KRL_PARSER.GetDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KUKA_KRL_PARSER"
        Background="#343638"
        mc:Ignorable="d"
        Title="GetDataWindow" Height="200" Width="400">
    <Grid>
        <Viewbox Stretch="Fill">
            <Canvas ClipToBounds="true" Width="400" Height="200">
                <Button Width="121" Height="20" Canvas.Left="45" Canvas.Top="80" Content="Path to EMI File" Click="btnOpen_Click"></Button>
                <TextBox Name="txtPath" Width="135" Canvas.Left="200" Canvas.Top="80"></TextBox>
                
                <Button Width="72" Height="20" Canvas.Left="45" Canvas.Top="150" Content="Submit" Click="Submit"></Button>
            </Canvas>
        </Viewbox>
    </Grid>
</Window>
